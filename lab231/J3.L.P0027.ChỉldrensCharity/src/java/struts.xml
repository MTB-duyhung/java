<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <!-- Configuration for the default package. -->
    <package name="default" extends="struts-default">
        <action name="index" class="action.HomeAction"> 
            <result>Home.jsp</result>          
        </action>
        <action name="aboutPage" class="action.AboutAction"> 
            <result name="success">About.jsp</result>          
        </action>
        <action name="viewPhoto" class="action.PhotoAction"> 
            <result name="success">Photo Album.jsp</result>          
        </action>
    </package>
</struts>
